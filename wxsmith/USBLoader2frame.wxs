<?xml version="1.0" encoding="utf-8" ?>
<wxsmith>
	<object class="wxFrame" name="USBLoader2Frame">
		<title>USB Zuendmodul Konfigurator</title>
		<size>500,600</size>
		<object class="wxPanel" name="ID_PANEL1" variable="Panel1" member="yes">
			<pos>184,200</pos>
			<object class="wxBoxSizer" variable="BoxSizer1" member="no">
				<orient>wxVERTICAL</orient>
				<object class="sizeritem">
					<object class="wxNotebook" name="ID_NOTEBOOK1" variable="Notebook1" member="yes">
						<focused>1</focused>
						<style>wxNB_FIXEDWIDTH|wxBORDER_RAISED</style>
						<object class="notebookpage">
							<object class="wxPanel" name="ID_PANEL2" variable="Panel2" member="yes">
								<pos>131,153</pos>
								<object class="wxBoxSizer" variable="BoxSizer2" member="no">
									<orient>wxVERTICAL</orient>
									<object class="sizeritem">
										<object class="wxStaticBoxSizer" variable="StaticBoxSizer1" member="no">
											<label>Funktion</label>
											<object class="sizeritem">
												<object class="wxRadioButton" name="ID_RADIOBUTTON1" variable="RadioButton1" member="yes">
													<label>Manuell</label>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxRadioButton" name="ID_RADIOBUTTON2" variable="RadioButton2" member="yes">
													<label>Fester Abstand von OT</label>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxRadioButton" name="ID_RADIOBUTTON3" variable="RadioButton3" member="yes">
													<label>Dynamisch von OT</label>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL|wxEXPAND</flag>
										<border>10</border>
									</object>
									<object class="sizeritem">
										<object class="wxFlexGridSizer" variable="FlexGridSizer1" member="no">
											<cols>3</cols>
											<rows>2</rows>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT2" variable="StaticText2" member="yes">
													<label>Zuendzeitpunkt:</label>
													<style>wxWANTS_CHARS</style>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxSpinCtrl" name="ID_SPINCTRL2" variable="SpinCtrl2" member="yes">
													<value>0</value>
													<min>-30</min>
													<max>20</max>
													<size>115,30</size>
													<tooltip>Grad von OT, bei dem gezündet wird = -22 Grad wäre 22 Grad vot OT</tooltip>
												</object>
												<flag>wxALL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT4" variable="StaticText4" member="yes">
													<label>Grad (Wert 0 = OT)</label>
												</object>
												<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT3" variable="StaticText3" member="yes">
													<label>Spule laden:</label>
													<tooltip>Wieviel Grad von einer Umdrehung für das magnetische Laden der Zünnspule genutzt werden (bei Unterbrecher-Nocken ca. 180°)</tooltip>
												</object>
												<flag>wxALL|wxALIGN_RIGHT|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxSpinCtrl" name="ID_SPINCTRL3" variable="SpinCtrl3" member="yes">
													<value>270</value>
													<min>90</min>
													<max>320</max>
													<size>115,30</size>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxStaticText" name="ID_STATICTEXT5" variable="StaticText5" member="yes">
													<label>Grad</label>
												</object>
												<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL</flag>
										<border>5</border>
									</object>
									<object class="sizeritem">
										<object class="wxStaticBoxSizer" variable="StaticBoxSizer2" member="no">
											<label>LED</label>
											<object class="sizeritem">
												<object class="wxRadioButton" name="ID_RADIOBUTTON4" variable="RadioButton4" member="yes">
													<label>Aus</label>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxRadioButton" name="ID_RADIOBUTTON5" variable="RadioButton5" member="yes">
													<label>Eingangssignal</label>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
											<object class="sizeritem">
												<object class="wxRadioButton" name="ID_RADIOBUTTON6" variable="RadioButton6" member="yes">
													<label>Diagnose</label>
												</object>
												<flag>wxALL|wxALIGN_CENTER_HORIZONTAL|wxALIGN_CENTER_VERTICAL</flag>
												<border>5</border>
												<option>1</option>
											</object>
										</object>
										<flag>wxALL|wxEXPAND</flag>
										<border>10</border>
									</object>
								</object>
							</object>
							<label>Basis</label>
						</object>
						<object class="notebookpage">
							<object class="wxPanel" name="ID_PANEL3" variable="Panel3" member="yes" />
							<label>Tabelle1</label>
						</object>
						<object class="notebookpage">
							<object class="wxPanel" name="ID_PANEL4" variable="Panel4" member="yes" />
							<label>Tabelle2</label>
						</object>
						<object class="notebookpage">
							<object class="wxPanel" name="ID_PANEL5" variable="Panel5" member="yes" />
							<label>Tabelle3</label>
						</object>
						<object class="notebookpage">
							<object class="wxPanel" name="ID_PANEL6" variable="Panel6" member="yes" />
							<label>Werte</label>
						</object>
					</object>
					<flag>wxALL|wxEXPAND</flag>
					<border>5</border>
					<option>2</option>
				</object>
				<object class="sizeritem">
					<object class="wxTextCtrl" name="ID_TEXTCTRL2" variable="TextCtrl2" member="yes">
						<style>wxTE_MULTILINE|wxTE_LEFT|wxBORDER_SUNKEN</style>
					</object>
					<flag>wxALL|wxEXPAND</flag>
					<border>5</border>
					<option>1</option>
				</object>
			</object>
		</object>
		<object class="wxMenuBar" variable="HauptMenuBar" member="no">
			<object class="wxMenu" variable="Menu1" member="no">
				<label>&amp;File</label>
				<object class="wxMenuItem" name="idMenuQuit" variable="MenuItem1" member="no">
					<label>Quit</label>
					<accel>Alt-F4</accel>
					<help>Quit the application</help>
					<handler function="OnQuit" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu3" member="no">
				<label>&amp;Kommunikation</label>
				<object class="wxMenuItem" name="idMenuVerbinden" variable="MenuItem3" member="yes">
					<label>Verbinden</label>
					<help>Verbinden Modul</help>
					<handler function="OnConnectUsb" entry="EVT_MENU" />
				</object>
				<object class="wxMenuItem" name="idMenuTrennen" variable="MenuItem4" member="yes">
					<label>Trennen</label>
					<help>Trennen Modul</help>
					<handler function="OnDisconnectUsb" entry="EVT_MENU" />
				</object>
			</object>
			<object class="wxMenu" variable="Menu2" member="no">
				<label>Help</label>
				<object class="wxMenuItem" name="idMenuAbout" variable="MenuItem2" member="no">
					<label>About</label>
					<accel>F1</accel>
					<help>Show info about this application</help>
					<handler function="OnAbout" entry="EVT_MENU" />
				</object>
			</object>
		</object>
		<object class="wxStatusBar" name="ID_STATUSBAR1" variable="StatusBar1" member="yes">
			<fields>2</fields>
			<widths>-1,-1</widths>
			<styles>wxSB_RAISED,wxSB_RAISED</styles>
		</object>
	</object>
</wxsmith>
